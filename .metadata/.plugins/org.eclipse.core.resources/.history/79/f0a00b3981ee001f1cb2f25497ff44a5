package app1;

public class E {
	    public static void main(String[] args) {
	        int n = 5; // Number of rows
	        
	        for (int i = 0; i < n; i++) {
	            int num = 5 + i; // Starting number for each row
	            
	            for (int j = 0; j < 9; j++) {
	                System.out.print(num);
	                
	                // Decrease till the middle, then increase
	                if (j < 4) {
	                    num--;
	                } else {
	                    num++;
	                }
	            }
	            System.out.println();
	        }
	    }
	

}
